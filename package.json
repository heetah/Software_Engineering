{
  "name": "project",
  "version": "1.0.0",
  "description": "Project Description",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "lint": "eslint . --ext .js,.html,.css,.json",
    "lint:fix": "eslint . --ext .js,.html,.css,.json --fix",
    "lint:js": "eslint . --ext .js",
    "lint:html": "eslint . --ext .html",
    "lint:css": "eslint . --ext .css",
    "lint:json": "eslint . --ext .json",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:js": "prettier --write \"**/*.{js,mjs,cjs}\"",
    "format:html": "prettier --write \"**/*.html\"",
    "format:css": "prettier --write \"**/*.{css,scss,sass}\"",
    "format:json": "prettier --write \"**/*.json\""
  },
  "devDependencies": {
    "electron": "^32.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-html": "^8.1.3",
    "eslint-plugin-json": "^4.0.1",
    "prettier": "^3.2.5"
  }
}
